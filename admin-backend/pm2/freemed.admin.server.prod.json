{
  "apps" : [{
    "name": "freemed.admin.server.prod",
    "script": "app.js",
    "ignore_watch" : ["node_modules", "logs"],
    "env": {
      "NODE_ENV": "production",
      "NODE_PATH": "src"
    },
    "merge_logs": true,
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "error_file": "./logs/freemed.recruit.prod.stderr.log",
    "out_file": "./logs/freemed.recruit.prod.stdout.log",
    "pid_file": "./logs/freemed.recruit.prod.pid"
  }]
}
